<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.myviewdemo.QQStepView
        android:id="@+id/stepView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:visibility="gone"
        app:innerColor="@color/colorPrimary"
        app:outerColor="@color/colorAccent"
        app:stepTextColor="@color/colorPrimaryDark"
        app:stepTextSize="40dp"
        app:stepWidth="10dp" />

    <include
        layout="@layout/layout_color_track_textview"
        android:visibility="gone" />

    <com.example.myviewdemo.InputNumberView
        android:id="@+id/inputnumber"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:visibility="gone" />

    <com.example.myviewdemo.FlowLayout
        android:id="@+id/flow_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.example.myviewdemo.KeyPadView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="4dp"
        android:visibility="gone"
        app:itemMargin="4dp"
        app:numSize="20sp" />

    <com.example.myviewdemo.SlideMenuView
        android:id="@+id/slideMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:function="delete|read">

        <TextView
            android:id="@+id/tv_content"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="#334499"
            android:gravity="center"
            android:text="新通知"
            android:textColor="@android:color/white"
            android:textSize="20sp" />
    </com.example.myviewdemo.SlideMenuView>

    <com.example.myviewdemo.WatchFaceView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:watchfaceBackground="@mipmap/watchface" />

    <com.example.myviewdemo.ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:progressTextColor="@android:color/black"
        app:progressTextSize="18sp"
        app:roundWidth="10dp" />

    <com.example.myviewdemo.ShapeView
        android:id="@+id/shapeView"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_centerInParent="true"
        android:visibility="gone" />

    <com.example.myviewdemo.RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_red_dark"
        android:visibility="gone" />

    <com.example.myviewdemo.LetterSlideBar
        android:id="@+id/letterSlideBar"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:visibility="gone" />

    <TextView
        android:id="@+id/letterSlideBarShow"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_centerInParent="true"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="E"
        android:textColor="@android:color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        android:visibility="gone" />

