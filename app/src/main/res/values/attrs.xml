<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color" />
        <attr name="innerColor" format="color" />
        <attr name="stepWidth" format="dimension" />
        <attr name="stepTextSize" format="dimension" />
        <attr name="stepTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ColorTrackTextView">
        <attr name="orignColor" format="color" />
        <attr name="changeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="InputNumberView">
        <attr name="maxValue" format="integer" />
        <attr name="minValue" format="integer" />
        <attr name="defValue" format="integer" />
        <attr name="step" format="integer" />
        <attr name="valueSize" format="dimension" />
        <attr name="btnBackground" format="reference" />
        <attr name="disable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LoginPageView">
        <attr name="mainColor" format="color" />
        <attr name="verifySize" format="integer" />
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="maxLine" format="integer" />
        <attr name="itemHorizontalMargin" format="dimension" />
        <attr name="itemVerticalMargin" format="dimension" />
        <attr name="textMaxLength" format="integer" />
        <attr name="textColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="KeyPadView">
        <attr name="itemMargin" format="dimension" />
        <attr name="numColor" format="color" />
        <attr name="numSize" format="dimension" />
        <attr name="itemNormalBg" format="color" />
        <attr name="itemPressBg" format="color" />
    </declare-styleable>
    <declare-styleable name="SlideMenuView">
        <attr name="function" format="flags">
            <flag name="delete" value="0x30" />
            <flag name="read" value="0x50" />
            <flag name="top" value="0x03" />
        </attr>
    </declare-styleable>
    <declare-styleable name="WatchFaceView">
        <attr name="hourColor" format="color" />
        <attr name="minColor" format="color" />
        <attr name="secondColor" format="color" />
        <attr name="scaleColor" format="color" />
        <attr name="watchfaceBackground" format="reference" />
        <attr name="scaleShow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ProgressBar">
        <attr name="innerBackground" format="color" />
        <attr name="outerBackground" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <attr name="starNormal" format="reference" />
        <attr name="starFocus" format="reference" />
        <attr name="starNum" format="integer" />

    </declare-styleable>
    <declare-styleable name="LetterSlideBar">
        <attr name="letterTextSizeNormal" format="dimension" />
        <attr name="letterTextSizePress" format="dimension" />
        <attr name="letterTextColorNormal" format="color" />
        <attr name="letterTextColorPress" format="color" />
        <attr name="letterBackgroundNormal" format="color" />
        <attr name="letterBackgroundPress" format="color" />

    </declare-styleable>
</resources>